def profil (nama, alamat, umur, gender, hobi):
    print ("Nama saya adalah", nama)
    print ("Saya tinggal di", alamat)
    print ("Umur saya", umur)
    print ("Saya adalah seorang", gender)
    print ("Hobi saya adalah", hobi)

profil ('Nur Suci Pramidha', 'Bogor', '18 tahun', 'wanita', 'bermain bulutangkis')

#fungsi untuk mencari nilai kelulusan
def nilai_kelulusan (nilai):
  if nilai < 60:
    return "Gagal"
  elif 61 <= nilai <= 70:
    return "Baik"
  elif 71 <== nilai <= 80:
    return "Sangat baik"
  elif 81 <= nilai <= 100:
    return "Istimewa"
  else:
    return "Nilai tidak valid"

 print(nilai_kelulusan (80))

def ganjil(n):
    for i in range(1, n+1, 2):
        print(i)

ganjil(100)
